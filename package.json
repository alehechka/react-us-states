{
  "name": "react-us-states",
  "version": "1.0.1",
  "description": "React Hooks extensions to states-us npm package.",
  "keywords": [
    "react",
    "reactjs",
    "hooks",
    "united states",
    "usa",
    "us",
    "states",
    "territories",
    "abbreviations"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src test",
    "prepare": "tsdx build"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Adam Lehechka (https://github.com/alehechka)",
  "module": "dist/react-us-states.esm.js",
  "repository": "git://github.com/alehechka/react-us-states.git",
  "bugs": {
    "url": "https://github.com/alehechka/react-us-states/issues"
  },
  "homepage": "https://github.com/alehechka/react-us-states#readme",
  "dependencies": {
    "states-us": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "husky": "^4.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tsdx": "^0.13.3",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "resolutions": {
    "**/@typescript-eslint/parser": "4.0.1",
    "**/@typescript-eslint/eslint-plugin": "4.0.1"
  },
  "publishConfig": {
    "registry": "https://registry-url"
  }
}
